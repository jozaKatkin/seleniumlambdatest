<suite name="SuiteParallelism" parallel="tests" thread-count="2">
    <test name="ChromeTest">
        <parameter name="browser" value="chrome" />
        <parameter name="version" value="95" />
        <parameter name="platform" value="Windows 10" />
        <classes>
            <class name="SimpleFormDemoTest" />
            <class name="DragAndDropSlidersTest" />
            <class name="InputFormSubmitTest" />
        </classes>
    </test>

    <test name="FirefoxTest">
        <parameter name="browser" value="firefox" />
        <parameter name="version" value="100" />
        <parameter name="platform" value="Windows 10" />
        <classes>
            <class name="SimpleFormDemoTest" />
            <class name="DragAndDropSlidersTest" />
            <class name="InputFormSubmitTest" />
        </classes>
    </test>

    <test name="EdgeTest">
        <parameter name="browser" value="edge" />
        <parameter name="version" value="95" />
<!--        <parameter name="platform" value="macOS Sierra" />-->
        <parameter name="platform" value="macOS Ventura" />
        <classes>
            <class name="SimpleFormDemoTest" />
            <class name="DragAndDropSlidersTest" />
            <class name="InputFormSubmitTest" />
        </classes>
    </test>

